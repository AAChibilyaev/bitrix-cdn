{
  "id": null,
  "title": "WebP Queue Dashboard",
  "tags": ["webp", "queue", "cdn"],
  "style": "dark",
  "timezone": "browser",
  "refresh": "5s",
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "panels": [
    {
      "id": 1,
      "title": "WebP Queue Size",
      "type": "stat",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
      "targets": [
        {
          "expr": "webp_queue_size{cdn=\"bitrix\"}",
          "legendFormat": "Queue Size"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "palette-classic"},
          "custom": {
            "hideFrom": {"legend": false, "tooltip": false, "vis": false}
          }
        }
      }
    },
    {
      "id": 2,
      "title": "WebP Conversion Rate",
      "type": "graph",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
      "targets": [
        {
          "expr": "rate(webp_images_converted_total{cdn=\"bitrix\"}[1m]) * 60",
          "legendFormat": "Images/min"
        }
      ]
    },
    {
      "id": 3,
      "title": "WebP Conversion Errors",
      "type": "stat",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
      "targets": [
        {
          "expr": "webp_conversion_errors_total{cdn=\"bitrix\"}",
          "legendFormat": "Total Errors"
        }
      ]
    },
    {
      "id": 4,
      "title": "WebP Compression Ratio",
      "type": "stat",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
      "targets": [
        {
          "expr": "webp_compression_ratio{cdn=\"bitrix\"}",
          "legendFormat": "Compression %"
        }
      ]
    }
  ]
}